@prefix core: <http://w3id.org/polifonia/ontology/core/> .
@prefix jams: <http://w3id.org/polifonia/ontology/jams/> .
@prefix mc: <http://w3id.org/polifonia/ontology/musical-composition/> .
@prefix mp: <http://w3id.org/polifonia/ontology/musical-performance/> .
@prefix pon-resource: <http://w3id.org/polifonia/resource/> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

pon-resource:Agent%2Fannotator_local_id a core:Agent ;
    rdfs:label "Paul Holland" ;
    jams:hasAnnotatorType pon-resource:annotator-expert-human ;
    jams:hasName "Paul Holland" .

<http://w3id.org/polifonia/resource/Agent/Composer/Giuseppe_Verdi> a core:Agent ;
    rdfs:label "Giuseppe Verdi" .

<http://w3id.org/polifonia/resource/Agent/Composer/John_Lennon> a core:Agent ;
    rdfs:label "John Lennon" .

<http://w3id.org/polifonia/resource/JAMSAnnotation/audio> a jams:JAMSAnnotation,
        jams:JAMSAudioAnnotation ;
    jams:hasValidityDuration "128.0"^^xsd:float ;
    jams:hasValidityStartingTime "0"^^xsd:int ;
    jams:includesObservation <http://w3id.org/polifonia/resource/JAMSObservation/A@0.0>,
        <http://w3id.org/polifonia/resource/JAMSObservation/C:min@0.0>,
        <http://w3id.org/polifonia/resource/JAMSObservation/G:min@64.0> .

pon-resource:Recording%2Frecording_local_id a mp:Recording ;
    mc:hasTitle "This is a title" ;
    mc:label "This is a title" ;
    mc:recordingOf pon-resource:MusicalComposition%2Fsong_local_id ;
    mp:performedBy <http://w3id.org/polifonia/resource/Agent/Performer/Pino_Scotto>,
        <http://w3id.org/polifonia/resource/Agent/Performer/Richard_Benson> ;
    mp:recordingContainedIn "Blue release" ;
    prov:wasDerivedFrom jams:foo .

pon-resource:Score%2Fscore_local_id jams:performedBy pon-resource:Agent%2Fartist_local_id .

jams:foo a jams:JAMSFile ;
    jams:identifiers [ ] ;
    jams:jamsVersion "0.3.4" ;
    jams:release "Blue release" ;
    prov:wasDerivedFrom "isophonics" ;
    prov:wasMemberOf <https://github.com/jonnybluesman/choco> .

<http://w3id.org/polifonia/resource/Agent/Performer/Pino_Scotto> a core:Agent ;
    rdfs:label "Pino Scotto" .

<http://w3id.org/polifonia/resource/Agent/Performer/Richard_Benson> a core:Agent ;
    rdfs:label "Richard Benson" .

<http://w3id.org/polifonia/resource/JAMSObservation/A@0.0> a jams:JAMSAudioObservation,
        jams:JAMSObservation,
        jams:ScoreKeyModeOccurrence ;
    rdfs:label "A" ;
    jams:hasDuration "128.0"^^xsd:float ;
    jams:ofChord <http://purl.org/ontology/chord/resource/A> ;
    jams:startsAtTime "0.0"^^xsd:float .

<http://w3id.org/polifonia/resource/JAMSObservation/C:min@0.0> a jams:JAMSAudioObservation,
        jams:JAMSObservation,
        jams:ScoreChordOccurrence ;
    rdfs:label "C:min" ;
    jams:hasDuration "64.0"^^xsd:float ;
    jams:ofChord <http://purl.org/ontology/chord/resource/C:min> ;
    jams:startsAtTime "0.0"^^xsd:float .

<http://w3id.org/polifonia/resource/JAMSObservation/G:min@64.0> a jams:JAMSAudioObservation,
        jams:JAMSObservation,
        jams:ScoreChordOccurrence ;
    rdfs:label "G:min" ;
    jams:hasDuration "128.0"^^xsd:float ;
    jams:ofChord <http://purl.org/ontology/chord/resource/G:min> ;
    jams:startsAtTime "64.0"^^xsd:float .

